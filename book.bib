@book{Anderson.1973,
 abstract = {{\textquotedbl}The techniques used by the National Weather Service (NWS) for making river and flood forecasts have been changing in recent years (Sittner, 1973). Conceptual watershed models are replacing previously used empirical procedures. In 1972 the Hydrologic Research Laboratory of the Office of Hydrology, NWS, prepared a technical memorandum entitled {\textquotedbl}National Weather Service River Forecast System, Forecast Procedures{\textquotedbl} (referred to as HYDR0-14 throughout this report) as a guide for the implementation of conceptual river forecasting models by field offices. HYDR0-14 describes the techniques and computer programs needed for developing operational river forecasts based on the use of a continuous conceptual watershed model from the processing of the basic data to the preparation of the forecasts. The procedures described in HYDR0-14 did not include techniques to model snow accumulation and snowmelt. This Technical Memorandum describes a conceptual model of the snow accumulation and ablation process and the associated computer subroutines and programs which enable the model to be used in conjunction with the National Weather Service River Forecast System (NWSRFS). Guidelines and methods for determining model parameter values for a given area are also presented. Even though the snow subroutines are written for use with the NWSRFS, the snow accumulation and ablation model itself can be used with almost any soil moisture accounting (rainfall-runoff relationship) and channel routing procedure. The output from the snow model would be the input to the soil moisture accounting procedure. The output from the snow model is snowpack outflow (snowmelt water and rainwater leaving the snowpack) plus rain that fell on bare ground{\textquotedbl}--Introduction.



Eric A. Anderson.



Includes bibliographical references.},
 author = {Anderson, Eric A.},
 year = {1973},
 title = {National Weather Service river forecast system: snow accumulation and ablation model},
 url = {https://repository.library.noaa.gov/view/noaa/13507},
 keywords = {Mathematical models;Research;Watersheds},
 volume = {17},
 series = {NOAA technical memorandum NWS HYDRO},
 file = {9c6265ab-3207-41e8-bc67-e6abd1d8cd38:C\:\\Users\\DELL\\AppData\\Local\\Swiss Academic Software\\Citavi 6\\ProjectCache\\q0eb3oswz50yv0cf6vmql748nuk966uf3xgf47gscxgc06\\Citavi Attachments\\9c6265ab-3207-41e8-bc67-e6abd1d8cd38.pdf:pdf}
}


